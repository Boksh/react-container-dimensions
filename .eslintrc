{
  "extends": "eslint-config-okonet",
  "env": {
    "browser": true,
    "mocha": true
  },
  "plugins": [
    "import"
  ],
  "settings": {
    "import/resolve": {
      "moduleDirectory": ["node_modules"]
    }
  },
    "rules": {
        "indent": ["error", 4],
        "react/jsx-filename-extension": 0,
        "react/no-find-dom-node": 0
    }
}
